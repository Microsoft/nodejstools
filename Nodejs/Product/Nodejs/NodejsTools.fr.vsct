        <Strings>
          <CommandName>Ouvrir la documentation</CommandName>
          <ButtonText>Ouvrir la &amp;documentation</ButtonText>
        </Strings>
      </Button>
      <Button guid="guidNodeToolsCmdSet" id="cmdidDocumentation" priority="0x004" type="Button">
